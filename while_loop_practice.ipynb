   "source": [
    "num=int(input(\"enter the number :- \"))\n",
    "c=num\n",
    "ans=1\n",
    "while(c!=1):\n",
    "    ans = c*ans\n",
    "    c=c-1\n",
    "print(\"factorial of {} is\".format(num),ans)"
   ]
  },
